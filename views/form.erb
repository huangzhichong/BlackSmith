<script type="text/javascript">
  function test() 
    { 
      xpathExpression = document.getElementById("inputXPath").value 
      alert(xpathExpression)
      var nodes = document.evaluate("//div[@id='example']"+xpathExpression, document, null,XPathResult.ANY_TYPE, null);

      try {
        var thisNode = nodes.iterateNext();
         
        while (thisNode) {
          // alert( thisNode.nodeName );
          div = document.createElement('div')
          div.style.backgroundColor='red'
          // div = thisNode.parentNode.appendChild(div)
          thisNode.parentElement.style.backgroundColor='yellow'
          thisNode.style.display = 'none'
          alert(thisNode.textContent)
          thisNode = nodes.iterateNext();

        } 
      }
      catch (e) {
        dump( 'Error: Document tree modified during iteration ' + e );
      }


    } 

</script>






